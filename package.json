{
  "name": "fonds-prevention-argile",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@10.15.1",
  "scripts": {
    "build": "next build",
    "start": "next start",
    "start:dev": "next dev",
    "lint": "next lint",
    "typecheck": "tsc --noEmit",
    "validate": "pnpm typecheck && pnpm lint && pnpm test && pnpm precommit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "db:start": "docker-compose up -d postgres",
    "db:stop": "docker-compose down",
    "db:reset": "docker-compose down -v && docker-compose up -d postgres",
    "db:push": "pnpm drizzle-kit push",
    "db:push:force": "pnpm drizzle-kit push --force",
    "db:studio": "pnpm drizzle-kit studio",
    "db:generate": "pnpm drizzle-kit generate",
    "db:seed": "tsx src/lib/database/seeds/seed.ts",
    "db:seed:clear": "CLEAR_DB=true tsx src/lib/database/seeds/seed.ts",
    "db:setup": "pnpm db:push && pnpm db:seed",
    "db:reset:full": "pnpm db:reset && sleep 5 && pnpm db:setup",
    "db:migrate": "pnpm drizzle-kit migrate",
    "clean": "rm -rf .next && pnpm start:dev",
    "fresh": "rm -rf .next node_modules pnpm-lock.yaml && pnpm install && pnpm start:dev",
    "prepare": "husky",
    "precommit": "talisman --githook pre-commit",
    "talisman:check": "talisman --scan"
  },
  "dependencies": {
    "@gouvfr/dsfr": "^1.14.1",
    "@sentry/nextjs": "^9.46.0",
    "@socialgouv/matomo-next": "^1.9.2",
    "argon2": "^0.44.0",
    "drizzle-orm": "^0.44.5",
    "next": "^15.2.3",
    "postgres": "^3.4.7",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "zod": "^3.25.20"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4.0.8",
    "@testing-library/jest-dom": "^6.8.0",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/node": "^22.13.5",
    "@types/pg": "^8.15.5",
    "@types/react": "^19.0.10",
    "@types/react-dom": "^19.0.4",
    "@vitejs/plugin-react": "^5.0.2",
    "@vitest/ui": "^3.2.4",
    "drizzle-kit": "^0.31.4",
    "eslint": "^9.21.0",
    "eslint-config-next": "^15.1.7",
    "husky": "^9.1.7",
    "jsdom": "^26.1.0",
    "postcss": "^8.5.3",
    "prettier": "^3.5.3",
    "tailwindcss": "^4.0.8",
    "ts-node": "^10.9.2",
    "tsx": "^4.20.5",
    "typescript": "^5.7.3",
    "vitest": "^3.2.4"
  },
  "engines": {
    "node": ">=22.0.0 <23.0.0"
  }
}
