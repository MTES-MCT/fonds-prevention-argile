# Configuration Talisman pour fonds-prevention-argile
fileignoreconfig:
  - filename: pnpm-lock.yaml
    checksum: 1d7bc3d4053cf5d0d1406217903cb377172857a918e56ad8067fac07b8cbb9e4
  - filename: package-lock.json
    checksum: ignore
  - filename: .env.example
    checksum: ignore
  - filename: .env.local.example
    checksum: ignore
  - filename: .github/workflows/ci.yml
    checksum: ignore
  - filename: src/lib/config/franceconnect.config.ts
    checksum: 6e6566e0bbd7d174407615d4d3e6bb034138d6b2c8a5ccfe45e9251eb22cfb70

# Seuil de détection élevé pour éviter les faux positifs
threshold: high

# Permettre les valeurs d'exemple utilisées dans la CI
allowed_patterns:
  - "API_KEY_EXAMPLE"
  - "123456"
